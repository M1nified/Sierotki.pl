{
  "name": "Sierotki.js",
  "short_name": "Sierotki.js",
  "start_url": "https://sierotki.pl",
  "display": "standalone",
  "theme_color": "#f0f0f0",
  "manifest_version": 2,
  "version": "1.0",
  "offline_enabled": true,
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "background"
  ],
  "app": {
    "browse_urls": [
      "https://sierotki.pl/"
    ],
    "launch": {
      "container": "tab",
      "web_url": "https://sierotki.pl/"
    },
    "urls": [
      "https://sierotki.pl/"
    ]
  },
  "icons": {
    "128": "images/icon/icon_128.png",
    "144": "images/icon/icon_144.png"
  }
}
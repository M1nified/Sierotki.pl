{
  "name": "Sierotki.js",
  "short_name": "Sierotki.js",
  "start_url": "https://sierotki.pl",
  "display": "standalone",
  "theme_color": "#f0f0f0",
  "manifest_version": 2,
  "version": "0.1",
  "offline_enabled": true,
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "background"
  ],
  "app": {
    "browse_urls": [
      "https://sierotki.pl/"
    ],
    "launch": {
      "container": "tab",
      "web_url": "https://sierotki.pl/"
    },
    "urls": [
      "*://sierotki.pl/"
    ]
  },
  "icons": [
    {
      "src": "images/icon/icon_144.png",
      "sizes": "144x144"
    }
  ]
}